{
  "name": "client-app",
  "version": "0.0.0",
  "private": true,
  "description": "Logging framework and viewer",
  "repository": "https://github.com/discourse/logster/",
  "license": "MIT",
  "author": "Sam Saffron",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
